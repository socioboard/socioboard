@{
    List<Domain.Socioboard.Domain.ArchiveMessage> allArchiveMessage = (List<Domain.Socioboard.Domain.ArchiveMessage>)(Model);
}

<div class="container-fluid">
    <div class="col-md-12" style="margin-top:25px">

        <div class="col-md-9">
            <div class="row">
                <h2>Archived Messages</h2>
                <div class="col-xs-12">
                    <div class="row">
                        <div class="col-lg-12 col-md-12">

                            @{foreach (Domain.Socioboard.Domain.ArchiveMessage item in allArchiveMessage)
                              {
                                  try
                                  {
                                      string icon = "";
                                      string onClick = "";
                                      string network = "";
                                      if (item.SocialGroup == "twitter")
                                      {
                                          icon = "twticon.png";
                                          onClick = "detailsdiscoverytwitter";
                                          network = "twitter";
                                      }
                                      else if (item.SocialGroup == "facebook")
                                      {
                                          icon = "fb_icon.png";
                                          onClick = "getFacebookProfiles";
                                          network = "facebook";
                                      }
                                          
                                <div class="panel panel-primary" id="messagetaskable_@item.Id">
                                    <div class="panel-heading">
                                        <div class="row" style="width:750px;">
                                            <div class="col-xs-3">
                                                <img title="@item.UserName" alt="@item.ProfileId" src="@item.ImgUrl" id="formprofileurl_@item.Id" onclick=""@onClick"(this.alt)" class="img-circle" data-toggle="modal" data-target="#profileModal">
                                                <img class="img-responsive img-circle" src="~/Themes/@System.Configuration.ConfigurationManager.AppSettings["domain"]/Contents/img/@icon" style="margin-left: 35px; margin-top: -20px;">
                                                <span id="screanName_@item.UserName">@item.UserName</span>
                                                 <span style="float: left">@item.CreatedDateTime</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel-body">

                                        <p id="msg_@item.Id">@item.Message</p>
                                    </div>




                                    <div class="panel-footer">
                                        @*<a href="#">
                                            <span class="pull-left">View Details</span></a>*@
                                        <span class="pull-right">

                                            @*<i id="task_@row["FromId"]" class="fa fa-thumb-tack fa-fw" onclick="opentask('@row["Message"]')"></i>*@
                                            <i id="CommentReply_@item.UserId" class="fa fa-reply fa-fw" onclick="opnCommentReply('@network.ToString()', '@item.MessageId.ToString()', '@item.ProfileId.ToString()')"></i>
                                            @*<i id="archiveMessage_@row["FromId"]" class="fa fa-folder-open fa-fw form-group" onclick="SaveArchive('@sorteddatacount','@network.ToString()','@row["ProfileId"].ToString()','@row["MessageId"].ToString()','@row["MessageDate"].ToString()')"></i>*@
                                        </span>
                                        <div class="clearfix"></div>

                                    </div>
                                </div>
                                         
                                  }
                                  catch (Exception)
                                  {

                                      throw;
                                  }



                              }
                            }

                            @* Looping End *@



                            <!-- Modal -->
                            <div class="modal fade" id="CommentReplyArchive" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                                <div class="modal-dialog">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <button id="closecommentpopup" type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                                           @* <img src="Contents/img/photo.png" class="img-circle">
                                            <img class="img-responsive img-circle" src="Contents/img/twticon.png" style="margin-left: 35px; margin-top: -20px;">*@
                                        </div>
                                        <div class="modal-body">
                                            <h4>LEAVE A COMMENT</h4>
                                            <textarea id="comment_text" class="form-control" rows="3" style="resize:none" placeholder="Write your message here"></textarea>
                                        </div>
                                        <div id="leaveCommentReply" class="modal-footer">
                                            <button type="button" class="btn btn-default ">Comment</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--end of model-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>